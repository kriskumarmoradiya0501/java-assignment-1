
import java.util.Scanner;

public class ass_1_1 {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);

        System.out.print("Enter number for find Factorial of it : ");

        float fac = s.nextFloat();
        float fact = 1.0f;
        for (int j=1;j<=fac;j++){
            fact *=j;
        }
        System.out.println("The factorial of "+fac+" is "+fact);
    }
}
